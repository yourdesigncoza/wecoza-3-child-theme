{
  "name": "wecoza-scripts",
  "version": "1.0.0",
  "description": "Scripts for WeCoza project management",
  "private": true,
  "scripts": {
    "create-issues": "node create-github-issues.js",
    "create-issues:dry": "DRY_RUN=true node create-github-issues.js",
    "create-issues:debug": "DEBUG=true node create-github-issues.js",
    "create-issues:debug-dry": "DEBUG=true DRY_RUN=true node create-github-issues.js",
    "sync-issues": "node sync-github-issues.js",
    "sync-issues:dry": "DRY_RUN=true node sync-github-issues.js",
    "sync-issues:debug": "DEBUG=true node sync-github-issues.js",
    "sync-issues:debug-dry": "DEBUG=true DRY_RUN=true node sync-github-issues.js",
    "sync-issues:local-to-github": "SYNC_DIRECTION=local-to-github node sync-github-issues.js",
    "sync-issues:github-to-local": "SYNC_DIRECTION=github-to-local node sync-github-issues.js",
    "delete-issues": "node delete-issues.js",
    "delete-issues:dry": "DRY_RUN=true node delete-issues.js"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7"
  }
}
